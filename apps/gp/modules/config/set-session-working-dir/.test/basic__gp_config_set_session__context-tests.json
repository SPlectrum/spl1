{
  "testSuite": "gp/config/set-session-working-dir basic tests",
  "description": "Test session working directory context setting functionality",
  "tests": [
    {
      "testName": "set temp directory context",
      "action": "gp/config/set-session-working-dir",
      "params": {
        "path": "/tmp/test-session-basic"
      },
      "expect": {
        "status": "success",
        "contextUpdated": true
      }
    },
    {
      "testName": "set relative path context", 
      "action": "gp/config/set-session-working-dir",
      "params": {
        "path": "test-relative-session"
      },
      "expect": {
        "status": "success",
        "contextUpdated": true
      }
    }
  ]
}